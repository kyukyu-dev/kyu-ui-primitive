{
  "name": "kyu-ui-primitive",
  "version": "0.0.0",
  "author": "kyukyu-dev",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.9.4",
  "workspaces": [
    "apps/*",
    "internal/*",
    "packages/*/*"
  ],
  "devDependencies": {
    "@chance/eslint": "^1.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "axe-core": "^4.10.3",
    "esbuild": "^0.25.9",
    "eslint": "^9.35.0",
    "glob": "^11.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsup": "^8.5.0",
    "typescript": "5.7.3",
    "vitest": "^3.2.4",
    "vitest-axe": "^1.0.0-pre.5"
  },
  "engines": {
    "node": ">=18"
  },
  "prettier": {
    "traillingComma": "es5",
    "printWidth": 100,
    "singleQuote": true
  },
  "scripts": {
    "build": "yarn workspaces foreach -pt --from './packages/**/*' run build",
    "lint": "yarn workspaces foreach -pt run lint",
    "types:check": "tsc --skipLibCheck --noEmit",
    "test": "vitest",
    "format": "prettier --write ."
  }
}
