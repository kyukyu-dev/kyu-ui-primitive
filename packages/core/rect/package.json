{
  "name": "@kyui/rect",
  "version": "0.0.0",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "publishConfig": {
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist.index.d.ts",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.mts",
          "default": "./dist/index.mjs"
        },
        "require": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        }
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint --max-warnings 0 src",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "build": "kyui-build"
  },
  "devDependencies": {
    "@kyui/builder": "workspace:*",
    "@kyui/eslint-config": "workspace:*",
    "@kyui/typescript-config": "workspace:*",
    "eslint": "^9.35.0",
    "typescript": "5.7.3"
  }
}
